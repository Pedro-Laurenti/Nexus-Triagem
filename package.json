{
    "name": "formulario-triagem",
    "version": "2.5.2",
    "description": "Formulário de triagem infantil para avaliações.",
    "main": "dist-electron/main/index.js",
    "author": "Pedro Laurenti",
    "license": "MIT",
    "private": false,
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
        }
    },
    "type": "module",
    "scripts": {
        "clean": "rimraf dist-electron && rimraf dist && rimraf out && rimraf release && rimraf test-results",
        "dev": "vite",
        "preview": "vite preview",
        "build": "npm run clean && tsc && vite build && electron-builder",
        "build:unpack": "npm run clean && vite build && electron-builder --dir",
        "build:win": "npm run clean && vite build && electron-builder --win",
        "build:mac": "npm run clean && electron-builder --mac",
        "build:linux": "vite build && electron-builder --linux"
    },
    "dependencies": {
        "@electron-toolkit/utils": "^3.0.0",
        "@react-input/mask": "^1.2.5",
        "autoprefixer": "^10.4.19",
        "electron-updater": "^6.2.1",
        "npm": "^10.8.2",
        "prop-types": "^15.8.1",
        "puppeteer": "^22.11.2",
        "react-router-dom": "^6.23.1",
        "sass": "^1.77.8",
        "tailwindcss": "^3.4.6",
        "vite-plugin-compression": "^0.5.1"
    },
    "devDependencies": {
        "@ckeditor/ckeditor5-build-classic": "^41.4.2",
        "@ckeditor/ckeditor5-react": "^7.0.0",
        "@types/react": "^18.2.64",
        "@types/react-dom": "^18.2.21",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.18",
        "ckeditor5": "^42.0.1",
        "electron": "^29.1.1",
        "electron-builder": "^24.13.3",
        "postcss": "^8.4.35",
        "postcss-import": "^16.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.4.2",
        "vite": "^5.1.5",
        "vite-plugin-electron": "^0.28.4",
        "vite-plugin-electron-renderer": "^0.14.5"
    }
}
